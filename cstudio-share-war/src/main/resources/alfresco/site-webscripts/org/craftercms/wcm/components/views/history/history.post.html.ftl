<div  id="acnVersionWrapper" class="cstudio-view history-view">

    <style type="text/css">
        .history-view .view-square-wrp table.history-tbl .c1 {
            width: 20px;
        }
        .history-view .view-square-wrp table.history-tbl .c2,
        .history-view .view-square-wrp table.history-tbl .c3 {
            width: 100px;
        }
        .history-view .view-square-wrp table.history-tbl .c4 {
            width: 65px;
        }
        .history-view .view-square-wrp table.history-tbl .c5 {
            width: 40px;
        }

        .history-view .view-square-wrp table.history-tbl .c6 {
		     display: inline-block;
             padding-left: 10px;
             width: 160px;
             word-wrap: break-word;
        }

        .history-view .view-square-wrp .division {
            border-top:1px #CCC dashed;
            margin: 4px 0;
        }
    </style>

    <h3 class="padBottom">Version History</h3>
	<p>
    	<div class="view-block description">
        	<!-- Click "View" to preview or "Restore" to make the item the current
        	In Progress version. You may also select two documents and compare them. -->
    	</div>
	</p>

    <div class="view-block info">
        <div class="shown-for" style="float:left;">History shown for: <span class="show-for-item status-icon"></span></div>
        <a href="javascript:" class="wipe-out-and-revert" style="float:right;"><!--Wipe out recent edits and revert to live--></a>
        <div style="clear:both"></div>
    </div>

    <div class="view-square-wrp">
        <div class="head">
            <table class="history-tbl history-head">
                <tr>
                    <td><div class="c1">&nbsp;</div></td>
                    <td><div class="c2">Version</div></td>
                    <td><div class="c3">Edit Time</div></td>
                    <td><div class="c4">Edited By</div></td>
                    <td><div class="c6">Comment</div></td>
                    <td><div class="c5">&nbsp;</div></td>
                </tr>
            </table>
        </div>
        <div class="body">
			<div class="history-listing">
            	<table class="history-tbl history-listing">
                	<tr>
                    	<td><div class="c1"><input type="checkbox" class="" /></div></td>
                    	<td><div class="c2"></div></td>
                    	<td><div class="c3"></div></td>
                    	<td><div class="c4"></div></td>
                        <td><div class="c6"></div></td>
                    	<td><div class="c5"><a href="javascript:">View</a> | <a 	href="javascript:">Restore</a></div></td>
                	</tr>
            	</table>
			</div>
            <div class="division"></div>
            <table class="history-tbl history-prev">

            </table>
        </div>
    </div>

    <div class="action-wrapper acnSubmitButtons">
        <input type="button" class="compare-checked" value="Compare Checked Versions" />
        <input type="button" class="close-button" value="Close" />
    </div>

</div>
